    "node": "6.2.2"
  },
  "homepage": "https://github.com/orYoffe/Chat#readme",
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-handlebars": "^3.0.0",
    "morgan": "~1.7.0",
    "nodemon": "^1.9.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.0",
